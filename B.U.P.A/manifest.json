{
  "manifest_version": 3,
  "name": "Base URI Path Append",
  "version": "1.0",
  "description": "Quickly append predefined paths to the base URL of current tab",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Base URI Path Append"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}